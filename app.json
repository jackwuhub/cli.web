{
  "pages": [
    "pages/mainPack/home/index"
  ],
  "subPackages": [
    {
      "root": "pages/homePack/",
      "name": "homePack",
      "pages": [
      ]
    }
  ],
  "preloadRule": {
    "pages/mainPack/home/index": {
      "network": "all",
      "packages": [
      ]
    }
  },
  "resolveAlias": {
    "@/*": "/*"
  },
  "lazyCodeLoading": "requiredComponents",
